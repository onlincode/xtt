<script  >
 
function render(data, type, row, meta){
let renderString = meta.settings.aoColumns[meta.col].renderString
if (!renderString) return data;
if (!data) return ""
const variables = {data,type,row,meta};
return parseRenderString(renderString,variables).split(",").join("<br>")
}

// Add an event listener for the 'popstate' event
 

function removeBracketString(str){
return str.replace(/[\[\]]/g, (match) => {
  if (match === '[') {
    return '.${'; // replace [ with x
  } else {
    return '}'; // replace ] with y
  } 
});
}

function parseRenderString (str, variables){ 
  str = removeBracketString(str)
  newStr = str.replace(/\${(\w+(\.\w+)*)}/g, (match, variableName) => {
    const keys = variableName.split('.');
    let value = variables;
    keys.forEach(key => {value = value[key];});
    return value;
    });
    if (newStr.indexOf("${") !== -1) {
      return parseRenderString(newStr, variables)
    } else {
      return newStr
    }
}
